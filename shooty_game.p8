function _init()
--starts first music track
music(0)

	player={
		sp=1,
		x=59,
		y=59,
		w=8,
		h=8,
		dx=0,
		dy=0,
		acc=0.5
	}
	
		friction=0.75

	
	end
-->8
function player_update()

	player.dx*=friction
	player.dy*=friction

	if btn(0) then 
			player.dx-=player.acc
		end
	if btn(1) then 
		player.dx+=player.acc
	end
	if btn(2) then 
		player.dy-=player.acc
	end
	if btn(3) then 
		player.dy+=player.acc
	end
	
	player.x+=player.dx
	player.y+=player.dy
end
